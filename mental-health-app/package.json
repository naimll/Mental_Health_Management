{
  "name": "mental-health-app",
  "version": "0.1.0",
  "private": true,
  "description": "A mental health application integrated with Argon Design System React",
  "scripts": {
    "start": "react-scripts start",
    "build": "cross-env PUBLIC_URL=/ react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "compile-sass": "sass src/assets/scss/argon-design-system-react.scss src/assets/css/argon-design-system-react.css",
    "minify-sass": "sass src/assets/scss/argon-design-system-react.scss src/assets/css/argon-design-system-react.min.css --style compressed"
  },
  "dependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^13.5.0",
    "ajv": "^8.16.0",
    "bootstrap": "4.6.2",
    "classnames": "2.3.2",
    "headroom.js": "0.12.0",
    "moment": "2.29.4",
    "nouislider": "15.4.0",
    "react": "^18.3.1",
    "react-datetime": "3.2.0",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.29.0",
    "react-scripts": "^5.0.1",
    "reactstrap": "8.10.0",
    "sass": "1.62.1",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@types/markerclustererplus": "2.1.33",
    "@types/react": "18.2.6",
    "cross-env": "^7.0.3",
    "eslint-plugin-flowtype": "8.0.3",
    "jquery": "3.7.0",
    "typescript": "^4.9.5"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "overrides": {
    "svgo": "3.0.2"
  }
}
